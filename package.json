{
  "name": "@iconicompany/eslint-config",
  "version": "1.0.2",
  "description": "ESLint configuration by iconicompany",
  "type": "module",
  "exports": {
    ".": "./eslint.config.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
