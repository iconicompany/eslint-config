  },
  "homepage": "https://github.com/iconicompany/eslint-config#readme",
  "dependencies": {
    "@eslint/js": "8.x",
    "eslint-config-eslint": "8.x",
    "eslint-plugin-big-number-rules": "^2.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "8.x"
